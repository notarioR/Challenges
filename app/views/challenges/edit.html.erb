<section class="center">
<h1> Edit </h1> 
<%= form_for [@challenge] do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :poster %>
    <%= f.file_field :poster %>
    <br>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <%= f.submit 'Save' %>
  <% end %>
</section>